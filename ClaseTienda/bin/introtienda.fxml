<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.media.MediaView?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MediaView fx:id="mediaTienda" fitHeight="200.0" fitWidth="200.0" />
      <HBox alignment="TOP_CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Button lineSpacing="10.0" mnemonicParsing="false" text="Play" />
            <Button lineSpacing="10.0" mnemonicParsing="false" text="Pause" />
            <Button lineSpacing="10.0" mnemonicParsing="false" text="Stop" />
         </children>
      </HBox>
   </children>
</VBox>
