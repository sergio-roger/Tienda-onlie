<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="468.0" prefWidth="1370.0" stylesheets="@../css/estilooriginal.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ControllerListaProducto">
   <children>
      <VBox prefHeight="417.0" prefWidth="628.0">
         <children>
            <Label alignment="CENTER" prefHeight="65.0" prefWidth="1406.0" styleClass="titulo" stylesheets="@../css/estilooriginal.css" text="LISTA DE PRODUCTOS" />
            <HBox prefHeight="107.0" prefWidth="1370.0">
               <children>
                  <Pane prefHeight="123.0" prefWidth="1361.0" styleClass="borde">
                     <children>
                       <ComboBox fx:id="cmbTemporada" layoutX="29.0" layoutY="14.0" prefHeight="36.0" prefWidth="196.0" promptText="Seleccione una opción" /> 
                       <Button fx:id="Ir_carrito" disable="true" layoutX="255.0" layoutY="14.0" onAction="#Cargar_vista" prefHeight="36.0" prefWidth="166.0" text="Ir a carrito de compras" />
                        <Button layoutX="1242.0" layoutY="7.0" mnemonicParsing="false" onAction="#CerrarSesion" prefHeight="45.0" prefWidth="105.0" text="Cerrar Sección">
                           <styleClass>
                              <String fx:value="btn" />
                              <String fx:value="rojo" />
                           </styleClass>
                        </Button>
                     </children>
                  </Pane>
               </children>
            </HBox>
           <TilePane fx:id="tileProductos" prefHeight="200.0" prefWidth="200.0" stylesheets="@../css/estilooriginal.css" />
         </children>
      </VBox>
   </children>
</VBox>
